def linear_search(n,x):
    element = []
    for i in range(1,n):
        element.append(i)
        
    count = 0
    flag = 0
    for i in element:
        count+=1
        if (i==x):
            print("i found the no at the location :" +str(i))
            flag = 1
            break;
            
    if (flag ==0):
        print("no not found")
    
    print("no of iterations :" + str(count))
